<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="350.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.pwn.reaktor.dziekanat.controller.SignInController">
   <children>
      <Label fx:id="lblSignInLogin" layoutX="65.0" layoutY="52.0" text="Login:" />
      <Label fx:id="lblSignInPassword" layoutX="65.0" layoutY="118.0" text="Password:" />
      <Label fx:id="lblSignInComparePassword" layoutX="66.0" layoutY="182.0" text="Confirm password" />
      <Button fx:id="btnShowPsw" layoutX="300.0" layoutY="144.0" mnemonicParsing="false" onMouseClicked="#showPsw" text="show" />
      <Button fx:id="btnShowConfPsw" layoutX="300.0" layoutY="212.0" mnemonicParsing="false" onMouseClicked="#showConfPsw" text="show" />
      <Button fx:id="btnSignIn" layoutX="108.0" layoutY="290.0" mnemonicParsing="false" onMouseClicked="#signIn" prefHeight="25.0" prefWidth="87.0" text="sign In" />
      <Button fx:id="btnCancell" layoutX="222.0" layoutY="290.0" mnemonicParsing="false" onMouseClicked="#cancelSignIn" prefHeight="25.0" prefWidth="87.0" text="cancel" />
      <TextField fx:id="tfLogin" layoutX="65.0" layoutY="85.0" prefHeight="25.0" prefWidth="206.0" />
      <TextField fx:id="tfPassword" layoutX="65.0" layoutY="144.0" prefHeight="25.0" prefWidth="206.0" visible="false" />
      <TextField fx:id="tfConfPassword" layoutX="66.0" layoutY="212.0" prefHeight="25.0" prefWidth="206.0" />
      <PasswordField fx:id="pfPassword" layoutX="66.0" layoutY="144.0" prefHeight="25.0" prefWidth="206.0" />
      <PasswordField fx:id="pfConfPassword" layoutX="65.0" layoutY="212.0" prefHeight="25.0" prefWidth="206.0" />
   </children>
</AnchorPane>
